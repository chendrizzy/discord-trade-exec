{
  "name": "discord-trade-executor-saas",
  "version": "1.0.0",
  "description": "Automated Trading Bot SaaS Platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:dashboard": "vite",
    "build": "vite build",
    "build:dashboard": "vite build",
    "preview:dashboard": "vite preview",
    "test": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --no-coverage --maxWorkers=4",
    "test:ci": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --ci --no-coverage --maxWorkers=2",
    "test:coverage": "c8 --reporter=text --reporter=lcov --reporter=html npm test",
    "test:coverage:critical": "c8 --reporter=text --reporter=lcov --include='src/routes/api/auth.js' --include='src/middleware/auth.js' --include='src/services/OAuth2Service.js' --include='src/services/MFAService.js' --include='src/services/billing/**' --include='src/services/RiskManagementService.js' --include='src/services/TradeExecutionService.js' npm test",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --testPathPatterns=tests/unit --no-coverage --maxWorkers=4",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --testPathPatterns=tests/integration --no-coverage --maxWorkers=2",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --testPathPatterns=tests/e2e --no-coverage --maxWorkers=1",
    "test:e2e:auth": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest tests/integration/auth-oauth.test.js --no-coverage",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=4096' jest --watch --no-coverage --maxWorkers=2",
    "test:visual": "playwright test tests/visual --reporter=html",
    "test:visual:update": "playwright test tests/visual --update-snapshots",
    "test:visual:components": "playwright test tests/visual/components --reporter=html",
    "test:visual:pages": "playwright test tests/visual/pages --reporter=html",
    "test:visual:responsive": "playwright test tests/visual/responsive --reporter=html",
    "test:visual:accessibility": "playwright test tests/visual --grep='Accessibility' --reporter=html",
    "test:visual:ui": "playwright test tests/visual --ui",
    "test:visual:debug": "playwright test tests/visual --debug",
    "test:a11y": "npm run test:a11y:axe && npm run test:a11y:pa11y",
    "test:a11y:axe": "playwright test --config=playwright-a11y.config.js",
    "test:a11y:pa11y": "pa11y-ci --config .pa11yci.json",
    "test:a11y:ci": "playwright test --config=playwright-a11y.config.js",
    "test:a11y:report": "node scripts/accessibility/generate-compliance-report.js",
    "lint": "eslint src/ tests/ --ext .js",
    "deploy": "railway up",
    "deploy:staging": "./scripts/deployment/deploy-staging.sh",
    "deploy:validate": "node scripts/deployment/validate-websocket-deployment.js",
    "deploy:validate:comprehensive": "VALIDATION_MODE=comprehensive node scripts/deployment/validate-websocket-deployment.js",
    "test:order-types:staging": "NODE_ENV=staging node scripts/deployment/validate-order-types.js",
    "test:rate-limit:staging": "NODE_ENV=staging node scripts/deployment/stress-test-rate-limits.js",
    "test:rate-limit:ibkr": "NODE_ENV=staging node -e \"require('./scripts/deployment/stress-test-rate-limits.js').testIBKR()\"",
    "test:rate-limit:schwab": "NODE_ENV=staging node -e \"require('./scripts/deployment/stress-test-rate-limits.js').testSchwab()\"",
    "test:rate-limit:alpaca": "NODE_ENV=staging node -e \"require('./scripts/deployment/stress-test-rate-limits.js').testAlpaca()\"",
    "monitoring:setup": "./scripts/deployment/setup-monitoring.sh",
    "db:backup:staging": "echo 'Database backup script not yet implemented'",
    "db:migrate:staging": "echo 'Database migration script not yet implemented'",
    "db:restore:staging": "echo 'Database restore script not yet implemented'",
    "health-check:staging": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.1.3",
    "@aws-sdk/client-kms": "^3.911.0",
    "@hookform/resolvers": "^5.2.2",
    "@playwright/test": "^1.55.0",
    "@polar-sh/sdk": "^0.37.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/node": "^10.21.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@stoqey/ib": "^1.5.1",
    "@tanstack/react-table": "^8.21.3",
    "aws": "^0.0.3-2",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.0.0",
    "ccxt": "^4.1.99",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "mongoose": "^8.0.4",
    "moomoo-api": "^9.4.5408",
    "natural": "^6.12.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "ora": "^9.0.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "playwright": "^1.55.0",
    "prompts": "^2.4.2",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^5.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.2.1",
    "redis": "^5.8.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mongodb-memory-server": "^10.2.1",
    "nock": "^14.0.10",
    "nodemon": "^3.0.2",
    "pa11y-ci": "^4.0.1",
    "pixelmatch": "^7.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.2.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.16",
    "vite": "^6.0.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=22.11.0"
  },
  "overrides": {
    "glob": "^10.4.5",
    "axios": "^1.12.2"
  },
  "c8": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/*.spec.js",
      "tests/**",
      "**/*.config.js",
      "**/*.setup.js"
    ],
    "reporter": [
      "text",
      "lcov",
      "html",
      "json-summary"
    ],
    "check-coverage": true,
    "lines": 75,
    "functions": 75,
    "branches": 70,
    "statements": 75,
    "per-file": false
  }
}
